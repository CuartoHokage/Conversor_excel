<form action="/api/upload_video" method="post" enctype="multipart/form-data">
            <h3 for="exampleFormControlInput1">Actualizar videos</h3>
            <div class="form-group">
              <label for="exampleFormControlSelect2">Elija zona</label>
                <select multiple class="form-control" name="zona" id="exampleFormControlSelect2">
                    <option name="id_imagen" value="Coral">Coral Hipermercados</option>
                    <option name="id_imagen" value="Centro_comercial">Centro Comercial</option>
                </select>
                <label for="exampleFormControlSelect2">Elija video que desea cambiar</label>
                
                <select multiple class="form-control" name="seleccion" id="exampleFormControlSelect2">
                    <option name="id_imagen" value="1">Video 1 </option>
                    <option name="id_imagen" value="2">Video 2 </option>
                    <option name="id_imagen" value="3">Video 3 </option>
                    <option name="id_imagen" value="4">Video 4 </option>
                    <option name="id_imagen" value="5">Video 5 </option>
                </select>
            </div>
            <div class="form-group">
                <label for="picture">Imagen del banner</label>
                <input type="file" class="form-control-file" name="picture" id="picture">
                <p id="texto"> </p><br />

            </div>
            <button type="submit" class="btn btn-primary" id="guardar">Subir video</button>
        </form>     
<hr>